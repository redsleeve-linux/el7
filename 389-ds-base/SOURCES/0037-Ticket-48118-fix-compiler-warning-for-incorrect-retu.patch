+static int32_t
 _cl5CheckCSNinCL(const ruv_enum_data *element, void *arg)
 {
     CL5DBFile *file = (CL5DBFile *)arg;
@@ -2739,7 +2739,7 @@ _cl5CheckCSNinCL(const ruv_enum_data *element, void *arg)
     return rc;
 }
 
-static int64_t
