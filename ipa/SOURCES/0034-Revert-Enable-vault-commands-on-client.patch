+        return self.api.Command.vault_archive_internal.NO_CLI
+
     def get_args(self):
         for arg in self.api.Command.vault_archive_internal.args():
             yield arg
@@ -813,6 +825,10 @@ class vault_retrieve(Local):
         ),
     )
 
