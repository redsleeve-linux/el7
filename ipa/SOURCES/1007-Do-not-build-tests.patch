From 6d6b0ff66d08c431b71901bcad3b65a95958dc7b Mon Sep 17 00:00:00 2001
From: Martin Kosek <mkosek@redhat.com>
Date: Mon, 15 Sep 2014 10:21:40 +0200
Subject: [PATCH] Do not build tests

Tests have been moved to other SRPM.
---
 Makefile | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/Makefile b/Makefile
index 4337259a040f98d9647357444ef3af4d5d0329e6..ef151de07d42f60c3dcba57e3b467bc446d11644 100644
--- a/Makefile
+++ b/Makefile
@@ -69,7 +69,7 @@ export CFLAGS
 #JAVA_STACK_SIZE ?= 8m
 #export JAVA_STACK_SIZE
 
-all: bootstrap-autogen server tests
+all: bootstrap-autogen server
 	@for subdir in $(SUBDIRS); do \
 		(cd $$subdir && $(MAKE) $@) || exit 1; \
 	done
@@ -106,7 +106,7 @@ client-autogen: version-update
 tests-man-autogen: version-update
 	cd ipatests/man; if [ ! -e Makefile ]; then ../../autogen.sh --prefix=/usr --sysconfdir=/etc --localstatedir=/var --libdir=$(LIBDIR); fi
 
-install: all server-install tests-install client-install
+install: all server-install client-install
 	@for subdir in $(SUBDIRS); do \
 		(cd $$subdir && $(MAKE) $@) || exit 1; \
 	done
-- 
2.5.5

